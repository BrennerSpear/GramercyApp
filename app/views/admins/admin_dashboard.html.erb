<body>

    <div id="wrapper">

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Dashboard</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-4">
                  
                </div>
                <div class="col-lg-8">
                    <!-- /.panel -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> Shoppers
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="dataTable_wrapper">
                                        <table class="table table-striped table-bordered table-hover" id="data-table">
                                            <thead>
                                                <tr>
                                                    <th>Email</th>
                                                    <th>name</th>
                                                    <th>username</th>
                                                    <th>picture</th>
                                                    <th>website</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            <% @shoppers.each do |shopper| %>
                                                <tr>
                                                    <td><%= shopper.email %></td>
                                                    <td><%= shopper.name %></td>
                                                    <td><%= shopper.nickname %></td>
                                                    <td class= "text-center"><%= image_tag shopper.image, width: "50px;" %></td>
                                                    <td><%= shopper.website %></td>
                                                </tr>
                                            <% end %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-8 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-4">
                    <!-- /.panel -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> Shoppers
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="dataTable_wrapper">
                                        <table class="table table-striped table-bordered table-hover" id="data-table">
                                            <thead>
                                                <tr>
                                                    <th>Post</th>
                                                    <th>name</th>
                                                    <th>username</th>
                                                    <th>picture</th>
                                                    <th>website</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            <% @shoppers.each do |shopper| %>
                                                <tr>
                                                    <td><%= shopper.email %></td>
                                                    <td><%= shopper.name %></td>
                                                    <td><%= shopper.nickname %></td>
                                                    <td class= "text-center"><%= image_tag shopper.image, width: "50px;" %></td>
                                                    <td><%= shopper.website %></td>
                                                </tr>
                                            <% end %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <div class="col-lg-8">
                    <!-- /.panel -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> Orders
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="dataTable_wrapper">
                                        <table class="table table-striped table-bordered table-hover" id="data-table">
                                            <thead>
                                                <tr>
                                                    <th>Brand</th>
                                                    <th>Shopper</th>
                                                    <th>Post</th>
                                                    <th>Â¢/like</th>
                                                    <th>$/follow</th>
                                                    <th>Max coupon</th>
                                                    <th>Time left to Post</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            <% @orders.each do |order| %>
                                                <tr>
                                                    <td><%= order.shop.brand.name %></td>
                                                <%if order.post.present? %>
                                                    <td><%=order.post.shopper.name %></td>
                                                    <td><%=image_tag order.post.image %></td>
                                                <% else %>
                                                    <td></td>
                                                    <td></td>
                                                <% end %>
                                                    <td><%= order.cents_per_like %></td>
                                                    <td><%= order.dollars_per_follow %></td>
                                                    <td><%= order.max_total_allowed %></td>
                                                    <td><%= %></td>
                                                </tr>
                                            <% end %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-8 -->
            </div>
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->

