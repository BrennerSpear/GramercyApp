<div class="row">
    <div class="col-lg-4">
    </div>
    <div class="col-lg-8">
        <!-- /.panel -->
        <div class="panel panel-default">
            <div class="panel-heading">
                <i class="fa fa-bar-chart-o fa-fw"></i> Shoppers
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="dataTable_wrapper">
                            <table class="table table-striped table-bordered table-hover" id="data-table">
                                <thead>
                                    <tr>
                                        <th>Email</th>
                                        <th>name</th>
                                        <th>username</th>
                                        <th>picture</th>
                                        <th>website</th>
                                    </tr>
                                </thead>
                                <tbody>
                                <% @shoppers.each do |shopper| %>
                                    <tr>
                                        <td><%= shopper.email %></td>
                                        <td><%= shopper.name %></td>
                                        <td><%= shopper.nickname %></td>
                                        <td class= "text-center"><%= image_tag shopper.image, width: "50px;" %></td>
                                        <td><%= shopper.website %></td>
                                    </tr>
                                <% end %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <!-- /.row -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-8 -->
</div>
<!-- /.row -->
